class ParkingTile {

    field int oiiai;

    constructor ParkingTile new() {
        return this;
    }

    method void print(String msg, int row) {
        do Output.moveCursor(7+row, 7);
        do Output.printString(msg);
        do msg.dispose();
        return;
    }

    method void activate(Player p) {
        do print("This is a parking tile", 0);
        do print("Nothing happens", 1);
        do Sys.wait(500);
        return;
    }
}
