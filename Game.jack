    method void printPlayerInfo(){
        var int index;
        var int id;
        var int balance;
        var Player player;
        let index = 0;
        while ( index < 4 ){
            let player = players[index];
            let id = player.getID();
            let balance = player.getBalance();
            do Output.moveCursor(1+(4*index), 35);
            do Output.printString(prtplayer);
            do Output.printInt(id+1);
            do Output.printString(prtmoney);
            do Output.printInt(balance);
            do Output.moveCursor(1+(4*index)+1, 35);
            do Output.printString(prtoutjail);
            do Output.printInt(player.getGetOutJails());
            let index = index + 1;
        }
        return;
    }

    function void drawPips(int number, int dice){
        //清空骰子內部
        do Screen.setColor(0);
        do Screen.drawRectangle(403+(50*dice), 203, 437+(50*dice), 237);

        do Screen.setColor(1);

        // 1. 中間點 (Center): (380, 220)
        // 適用於: 1, 3, 5
        if ((number = 1) | (number = 3) | (number = 5)) {
            do Screen.drawCircle(420+(50*dice), 220, 3);
        }

        // 2. 左上 (370, 210) 與 右下 (390, 230)
        // 適用於: 2, 3, 4, 5, 6 (除了 1 以外)
        if (~(number = 1)) {
            do Screen.drawCircle(410+(50*dice), 210, 3);
            do Screen.drawCircle(430+(50*dice), 230, 3);
        }

        // 3. 右上 (390, 210) 與 左下 (370, 230)
        // 適用於: 4, 5, 6
        if ((number = 4) | (number = 5) | (number = 6)) {
            do Screen.drawCircle(430+(50*dice), 210, 3);
            do Screen.drawCircle(410+(50*dice), 230, 3);
        }

        // 4. 左中 (370, 220) 與 右中 (390, 220)
        // 適用於: 6
        if (number = 6) {
            do Screen.drawCircle(410+(50*dice), 220, 3);
            do Screen.drawCircle(430+(50*dice), 220, 3);
        }
        
        return;
    }

    function void drawDice(){
        // 第一顆骰子
        // 畫出骰子外框
        do Screen.setColor(1);
        do Screen.drawRectangle(400, 200, 440, 240);
        // 骰子內部填充白色，潤飾外角
        do Screen.setColor(0);
        do Screen.drawRectangle(402, 202, 438, 238);
        do Screen.drawPixel(400, 200);
        do Screen.drawPixel(400, 240);
        do Screen.drawPixel(440, 200);
        do Screen.drawPixel(440, 240);
        //潤飾內角
        do Screen.setColor(1);
        do Screen.drawPixel(402, 202);
        do Screen.drawPixel(402, 238);
        do Screen.drawPixel(438, 202);
        do Screen.drawPixel(438, 238);
        // 第二顆骰子
        // 畫出骰子外框
        do Screen.setColor(1);
        do Screen.drawRectangle(450, 200, 490, 240);
        // 骰子內部填充白色，潤飾外角
        do Screen.setColor(0);
        do Screen.drawRectangle(452, 202, 488, 238);
        do Screen.drawPixel(450, 200);
        do Screen.drawPixel(450, 240);
        do Screen.drawPixel(490, 200);
        do Screen.drawPixel(490, 240);
        //潤飾內角
        do Screen.setColor(1);
        do Screen.drawPixel(452, 202);
        do Screen.drawPixel(452, 238);
        do Screen.drawPixel(488, 202);
        do Screen.drawPixel(488, 238);
        return;
    }

    function int rollDice() {
        var int n;
        var int pips;
        do Output.moveCursor(19, 35);
        do Output.printString(prtroll1);
        do Output.moveCursor(20, 35);
        do Output.printString(prtroll2);
        do Output.moveCursor(21, 35);
        do Output.printString(prtroll3);
        let n=0;
        while (~(Keyboard.keyPressed())) {
            let n=n+1;
            if (n > 6) {
                let n=1;
            }
            do Game.drawPips(n, 0);
        }
        let pips = n;
        do Screen.setColor(0);
        do Screen.drawRectangle(280, 205, 370, 250);
        do Output.moveCursor(20, 35);
        do Output.printString(prtwait);
        do Sys.wait(500);
        do Screen.setColor(0);
        do Screen.drawRectangle(280, 205, 370, 250);
        do Output.moveCursor(19, 35);
        do Output.printString(prtroll1);
        do Output.moveCursor(20, 35);
        do Output.printString(prtroll2);
        do Output.moveCursor(21, 35);
        do Output.printString(prtroll3);
        while (~(Keyboard.keyPressed())) {
            let n=n+1;
            if (n > 6) {
                let n=1;
            }
            do Game.drawPips(n, 1);
        }
        let pips = pips + n;
        do Screen.setColor(0);
        do Screen.drawRectangle(280, 205, 370, 250);
        do Output.moveCursor(20, 35);
        do Output.printString(prtget);
        do Output.printInt(pips);
        do Output.printString(prtpips);
        return (pips);
    }

