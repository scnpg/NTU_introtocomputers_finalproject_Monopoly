    method void start() {
        var boolean end; // 遊戲結束
        var int pips; // 點數
        var Player currentPlayer; // 當下玩家
        var int nowIndex; // 當下玩家編號-1
        var int pos; // 當下玩家位置
        var Player draw;
        var int tmp;
        var int tmpx;
        var int tmpy;
        let tmp=0;
        let nowIndex = 0;
        let end = false;
        let currentPlayer = players[nowIndex];
        do printPlayerInfo();
        do Game.drawDice();
        do Game.drawMap();
        while (tmp<4){
            let tmpx = currentPlayer.getxCoordinate(0);
            let tmpy = currentPlayer.getyCoordinate(0);
            do Player.drawPlayer(tmp, tmpx, tmpy);
            let tmp = tmp+1;
        }
        while (~end){
            let currentPlayer = players[nowIndex];

            do Output.moveCursor(5, 8);
            do Output.printString(prtplayer);
            do Output.printInt(nowIndex+1);
            do Output.printString(prtturn);

            let pips = Game.rollDice();

            do currentPlayer.move(pips, 40);
			do Sys.wait(500);

            do board.handleLanding(currentPlayer);
            do Sys.wait(300);

			do Screen.clearScreen();
	        do Game.drawMap();
            do printPlayerInfo();
            do Game.drawDice();

            let tmp = 0;
            while (tmp<4){
                let draw = players[tmp];
                let pos = draw.getLocation();
                let tmpx = draw.getxCoordinate(pos);
                let tmpy = draw.getyCoordinate(pos);
                do Player.drawPlayer(tmp, tmpx, tmpy);
                let tmp = tmp+1;
            }

            do Sys.wait(500);

            let nowIndex = nowIndex+1;
            if (nowIndex = 4){
                let nowIndex = 0;
            }
            // if (bankruptcy){ //未完成
            //     end=ture;
            // }
        }
        return;
    }
    
